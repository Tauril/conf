XTerm*faceName: xft:DejaVu Sans Mono for Powerline:style=Book:size=10
XTerm*utf8: 2
XTerm*selectToClipboard: true

*foreground:   #c5c8c6
*background:   #1d1f21
*cursorColor:  #aeafad
*color0:       #000000

! git current branch
*color1:       #c12226

! No error
*color2:       #778900

! git repo not clean
*color3:       #ae7b00

! Time
*color4:       #1d5594

*color5:       #682a9b

! Current path
*color6:       #2b7751

*color7:       #929593

*color8:       #666666

! Error code
*color9:       #a66666

*color10:      #b5bd68

*color11:      #f0c674

! Directory
*color12:      #81a2be

! png, etc
*color13:      #b294bb

*color14:      #8abeb7

*color15:      #ecebec

XTerm.vt100.translations: #override \n\
  Ctrl <Key> minus: smaller-vt-font() \n\
  Ctrl <Key> plus: larger-vt-font()
